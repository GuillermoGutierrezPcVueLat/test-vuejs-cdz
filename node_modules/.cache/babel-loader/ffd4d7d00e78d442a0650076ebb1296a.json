 // This is going to be called \n  //  when the component gets rendered\n  created() {\n    this.getChat();\n  },\n  methods: {\n    onRegister(event, name) {\n      event.preventDefault();\n    // Authentication is out of scope for this project\n    // so we just generate a uuid\n    this.user = { name, id: uid() };\n    },\n    getChat() {\n      listenChat((chat) => {\n        this.messages = chat.reverse().map(m => ({\n          ...m,\n          isMine: m.uid && m.uid === this.user?.id\n        }));\n      });\n    },\n    // This method will be called when a new message is sent\n    onSubmit(event, text) {\n      event.preventDefault();\n\n      sendMessage({\n        text,\n        uid: this.user?.id,\n        author: this.user?.name\n      });\n    }\n  },\n  data: () => ({\n    user: undefined,\n    messages: []\n  })\n}\n</script>\n\n<style>\n@font-face {\n  font-family: 'Georama';\n  src: url('./assets/Georama.ttf');\n}\n\n@font-face {\n  font-family: 'Georama';\n  src: url('./assets/Georama.ttf');\n  font-weight: bold;\n}\n\n* {\n  box-sizing: border-box;\n}\n\nhtml {\n  font-family: 'Georama', sans-serif;\n}\n\nbody {\n  margin: 0;\n}\n\nbutton {\n  border: 0;\n  background: #2a60ff;\n  color: white;\n  cursor: pointer;\n  padding: 1rem;\n}\n\ninput {\n  border: 0;\n  padding: 1rem;\n  background: rgba(0, 0, 0, 0.1);\n}\n</style>\n\n<style scoped>\n.app {\n  height: 100vh;\n  display: flex;\n  flex-direction: column;\n}\n\n.messages {\n  flex-grow: 1;\n  overflow: auto;\n  padding: 1rem;\n}\n\n.message + .message {\n  margin-top: 1rem;\n}\n\n.message.right {\n  margin-left: auto;\n}\n</style>"]}]}