      return this.comment.usuario !== \"\" && this.comment.comentario !== \"\";\n        },\n        handleInputChange() {\n            \n        },\n        renderError(){},\n        setError: function(argT) {\n            this.error=argT;\n        },\n        loadComentarios(){\n          axiosInstance.get(\"api/mensajes\").then(res => {\n            console.log(res.data);\n            this.comments(res.data);\n            this.loading = false;\n          })\n        }\n    }\n    \n})\n</script>\n<style>\n\n</style>"]}]}